{
    "Environment": 0,
    "Modules": {
        "System":   {"Listen": "localhost:8081", "DB": "System"}
    },
    "FileServers": {
        "Misc":     {"Listen": "localhost:8082", "Path": "/", "Root": "{HomeDir}/public"}
    },
    "DB": {
